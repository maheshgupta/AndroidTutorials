<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.tutorials.andorid.app.HomeActivity">

    <Button
        android:id="@+id/btnWebViewExample"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="showWebViewExample"
        android:text="@string/web_view" />

    <Button
        android:id="@+id/btnOpenCamera"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="showCamera"
        android:text="@string/open_camera" />

    <Button
        android:id="@+id/btnShowProfilePage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="showProfilePage"
        android:text="@string/show_profile_page" />

</LinearLayout>
